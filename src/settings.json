                },
                {
                    "name": "move",
                    "arguments": ["switch/sphaira/sphaira.nro", "hbmenu.nro"]
                },
                {
                    "name": "delete",
                    "arguments": ["switch/sphaira"]
                }
            ]
        },
        "ascenttoolbox": {
