    "moduleList": {
        "atmosphere": {
            "repo": "Atmosphere-NX/Atmosphere",
            "regex": [
                ".*atmosphere.*\\.zip"
            ],
            "steps": [
                {
                    "name": "extract",
                    "arguments": [
                        ".*atmosphere.*\\.zip"
                    ]
                },
                {
                    "name": "delete",
                    "arguments": [
                        "switch/reboot_to_payload.nro"
                    ]
                },
                {
                    "name": "create_dir",
                    "arguments": [
                        "atmosphere/contents"
                    ]
                },
                {
                    "name": "copy",
                    "arguments": [
                        "atmosphere/config_templates/system_settings.ini",
                        "atmosphere/config/system_settings.ini"
                    ]
                },
                {
                    "name": "replace_content",
                    "arguments": [
                        "atmosphere/config/system_settings.ini",
                        "; dmnt_cheats_enabled_by_default = u8!0x1",
                        "dmnt_cheats_enabled_by_default = u8!0x0"
                    ]
                },
                {
                    "name": "replace_content",
                    "arguments": [
                        "atmosphere/config/system_settings.ini",
                        "; dmnt_always_save_cheat_toggles = u8!0x0",
                        "dmnt_always_save_cheat_toggles = u8!0x1"
                    ]
                }
            ]
        },
        "hekate": {
            "repo": "CTCaer/hekate",
            "regex": [
                ".*hekate.*\\.zip"
            ],
            "steps": [
                {
                    "name": "extract",
                    "arguments": [".*hekate.*\\.zip"]
                },
                {
                    "name": "copy",
                    "arguments": ["bootloader/hekate_*", "bootloader/update.bin"]
                },
                {
                    "name": "createDir",
                    "arguments": ["atmosphere"]
                },
                {
                    "name": "copy",
                    "arguments": ["bootloader/hekate_*", "atmosphere/reboot_payload.bin"]
                }
            ]
        },
        "ascentassets": {
            "repo": "exploitz86/Ascent-Assets",
