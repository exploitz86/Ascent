                    "name": "extract",
                    "arguments": [".*emuiibo.*\\.zip"]
                },
                {
                    "name": "delete",
                    "arguments": ["SdOut/atmosphere/contents/0100000000000352/flags/boot2.flag"]
                },
                {
                    "name": "move",
                    "arguments": ["SdOut/atmosphere", "atmosphere"]
                },
                {
                    "name": "move",
                    "arguments": ["SdOut/emuiibo", "emuiibo"]
                },
                {
                    "name": "move",
                    "arguments": ["SdOut/switch", "switch"]
                },
                {
                    "name": "delete",
