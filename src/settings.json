                },
                {
                    "name": "delete",
                    "arguments": ["README.md"]
                },
                {
                    "name": "delete",
                    "arguments": ["atmosphere/contents/00FF0000636C6BFF/flags/boot2.flag"]
                },
                {
                    "name": "create_file",
                    "arguments": ["atmosphere/contents/00FF0000636C6BFF/toolbox.json", "{\"name\": \"sys-clk\",\"tid\": \"00FF0000636C6BFF\",\"requires_reboot\": true}"]
                }
            ]
        },
